{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <img width="26" height="28" alt="ClientContext" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/>
    <span class="sf-toolbar-status">{{ collector.clientContextShort }}</span>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Context</b>
        <span>{{ collector.clientContextLong }}</span>
    </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}
